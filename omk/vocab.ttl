@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ev: <http://example.org/eval#> .

<http://example.org/eval#> a owl:Ontology ;
    dcterms:title "Eval"@fr ;
    dcterms:description "Vocabulaire simplifié : Etudiant, Cours, Enseignant et Note."@fr .

### Classes (noms en français dans les IRIs)
ev:Etudiant a rdfs:Class ;
    rdfs:label "Etudiant"@fr .

ev:Cours a rdfs:Class ;
    rdfs:label "Cours"@fr .

ev:Enseignant a rdfs:Class ;
    rdfs:label "Enseignant"@fr .

ev:Note a rdfs:Class ;
    rdfs:label "Note"@fr .

### Propriétés de données (noms en français)
ev:nom a rdf:Property ;
    rdfs:label "nom"@fr ;
    rdfs:domain ev:Etudiant , ev:Enseignant , ev:Cours ;
    rdfs:range xsd:string .

ev:courriel a rdf:Property ;
    rdfs:label "courriel"@fr ;
    rdfs:domain ev:Etudiant , ev:Enseignant ;
    rdfs:range xsd:string .

ev:code a rdf:Property ;
    rdfs:label "code"@fr ;
    rdfs:domain ev:Cours ;
    rdfs:range xsd:string .

ev:credits a rdf:Property ;
    rdfs:label "credits"@fr ;
    rdfs:domain ev:Cours ;
    rdfs:range xsd:integer .

ev:noteValeur a rdf:Property ;
    rdfs:label "noteValeur"@fr ;
    rdfs:domain ev:Note ;
    rdfs:range xsd:decimal .

### Propriétés d'objet (noms en français)
ev:recoit a rdf:Property ;
    rdfs:label "recoit"@fr ;
    rdfs:domain ev:Etudiant ;
    rdfs:range ev:Note ;
    rdfs:comment "L'Etudiant reçoit une note"@fr .

ev:contient a rdf:Property ;
    rdfs:label "contient"@fr ;
    rdfs:domain ev:Cours ;
    rdfs:range ev:Note ;
    rdfs:comment "Le cours contient des notes"@fr .

ev:enseigne a rdf:Property ;
    rdfs:label "enseigne"@fr ;
    rdfs:domain ev:Enseignant ;
    rdfs:range ev:Cours ;
    rdfs:comment "L'enseignant enseigne le cours"@fr .



