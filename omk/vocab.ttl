@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ev: <http://example.org/eval#> .

<http://example.org/eval#> a owl:Ontology ;
    dcterms:title "Eval"@fr ;
    dcterms:description "Vocabulaire simplifié : Etudiant, Cours, Enseignant et Note."@fr .

### Classes (noms en français dans les IRIs)
ev:Etudiant a rdfs:Class ;
    rdfs:label "Etudiant"@fr .

ev:Cours a rdfs:Class ;
    rdfs:label "Cours"@fr .

ev:Enseignant a rdfs:Class ;
    rdfs:label "Enseignant"@fr .

ev:Note a rdfs:Class ;
    rdfs:label "Note"@fr .

### Propriétés de données (noms en français)
ev:nom a rdf:Property ;
    rdfs:label "nom"@fr ;
    rdfs:domain ev:Etudiant , ev:Enseignant , ev:Cours ;
    rdfs:range xsd:string .

ev:courriel a rdf:Property ;
    rdfs:label "courriel"@fr ;
    rdfs:domain ev:Etudiant , ev:Enseignant ;
    rdfs:range xsd:string .

ev:code a rdf:Property ;
    rdfs:label "code"@fr ;
    rdfs:domain ev:Cours ;
    rdfs:range xsd:string .

ev:credits a rdf:Property ;
    rdfs:label "credits"@fr ;
    rdfs:domain ev:Cours ;
    rdfs:range xsd:integer .

ev:noteValeur a rdf:Property ;
    rdfs:label "noteValeur"@fr ;
    rdfs:domain ev:Note ;
    rdfs:range xsd:decimal .

### Propriétés d'objet (noms en français)

# Nouvelle entité : Inscription lie un Etudiant et un Cours et porte la note
ev:Inscription a rdfs:Class ;
    rdfs:label "Inscription"@fr .

ev:estInscrit a rdf:Property ;
    rdfs:label "estInscrit"@fr ;
    rdfs:domain ev:Etudiant ;
    rdfs:range ev:Inscription ;
    rdfs:comment "Relation : l'étudiant a une inscription"@fr .

ev:pourCours a rdf:Property ;
    rdfs:label "pourCours"@fr ;
    rdfs:domain ev:Inscription ;
    rdfs:range ev:Cours ;
    rdfs:comment "Relation : l'inscription concerne un cours"@fr .

ev:contientInscription a rdf:Property ;
    rdfs:label "contientInscription"@fr ;
    rdfs:domain ev:Cours ;
    rdfs:range ev:Inscription ;
    rdfs:comment "Relation : le cours contient des inscriptions"@fr .

ev:enseigne a rdf:Property ;
    rdfs:label "enseigne"@fr ;
    rdfs:domain ev:Enseignant ;
    rdfs:range ev:Cours ;
    rdfs:comment "L'enseignant enseigne le cours"@fr .



